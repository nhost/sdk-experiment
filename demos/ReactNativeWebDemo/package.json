{
  "name": "reactnativewebdemo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "test": "pnpm test:audit && pnpm test:typecheck && pnpm test:lint && pnpm test:format && echo \"No tests specified\" && exit 0",
    "test:typecheck": "tsc --noEmit",
    "test:lint": "eslint . && expo lint",
    "test:format": "prettier --check .",
    "test:audit": "pnpm audit",
    "format": "prettier --write .",
    "build": "pnpm expo export -p ios -p android",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint"
  },
