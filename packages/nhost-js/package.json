{
  "name": "nhost-js",
  "version": "1.0.0",
  "description": "Nhost JavaScript SDK",
  "main": "./dist/nhost-js.cjs.js",
  "module": "./dist/nhost-js.es.js",
  "types": "./dist/nhost-js.d.ts",
  "exports": {
    ".": {
      "types": "./dist/nhost-js.d.ts",
      "node": "./dist/nhost-js.cjs.js",
      "import": "./dist/nhost-js.es.js",
      "require": "./dist/nhost-js.cjs.js",
      "default": "./dist/nhost-js.umd.js"
    },
    "./auth": {
      "types": "./dist/nhost-js/auth.d.ts",
      "node": "./dist/nhost-js/auth.cjs.js",
      "import": "./dist/nhost-js/auth.es.js",
      "require": "./dist/nhost-js/auth.cjs.js",
      "default": "./dist/nhost-js/auth.js"
    },
    "./storage": {
      "types": "./dist/storage.d.ts",
      "node": "./dist/storage.cjs.js",
      "import": "./dist/storage.es.js",
      "require": "./dist/storage.cjs.js",
      "default": "./dist/storage.js"
    },
    "./graphql": {
      "types": "./dist/graphql.d.ts",
      "node": "./dist/graphql.cjs.js",
      "import": "./dist/graphql.es.js",
      "require": "./dist/graphql.cjs.js",
      "default": "./dist/graphql.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "vite build",
    "generate": "orval --config build/orval.config.ts",
    "prepublishOnly": "pnpm build",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "nhost",
    "sdk",
    "api",
    "javascript",
    "typescript"
  ],
  "author": "Nhost",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhost/nhost.git"
  },
  "homepage": "https://nhost.io",
  "bugs": {
    "url": "https://github.com/nhost/nhost/issues"
  },
  "packageManager": "pnpm@10.9.0",
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/rollup-plugin-peer-deps-external": "^2.2.5",
    "jest": "^29.7.0",
    "orval": "7.9.0",
    "prettier": "^3.5.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "terser": "^5.39.0",
    "ts-jest": "^29.3.2",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3"
  },
  "sideEffects": false
}
