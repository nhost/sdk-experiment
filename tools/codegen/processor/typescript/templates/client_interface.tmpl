{{- define "client_interface" }}
export interface Client {
{{- range .Methods }}
  {{- $methodName := .Name }}
  {{ $methodName }}(
    {{- range .PathParameters }}
    {{ .Name }}: {{ .Type.Name }},
    {{- end }}
    // body
    {{- if .HasQueryParameters }}
    params?: {{ title $methodName }}Params,
    {{- end }}
    options?: RequestInit,
  ): Promise<fixme>;
{{ end -}}
};
{{- end }}
