{{- define "client_interface" }}
export interface Client {
{{- range .Methods }}
  {{- $method := . }}
  {{ $method.Name }}(
    {{- range .PathParameters }}
    {{ .Name }}: {{ .Type.Name }},
    {{- end }}
    {{- range $code, $type := .Bodies }}
    body{{ if not $method.BodyRequired }}?{{ end }}: {{ $type.Name }},
    {{- end }}
    {{- if .HasQueryParameters }}
    params?: {{ title $method.Name }}Params,
    {{- end }}
    options?: RequestInit,
  ): Promise<fixme>;
{{ end -}}
};
{{- end }}
