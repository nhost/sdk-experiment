{{- define "renderObjectAttributeHelp" -}}
  {{ .Type.Schema.Schema.Description }}
  {{- if example .Type }}
    *    Example - {{ example .Type }}
  {{- end }}
  {{- if pattern .Type }}
    *    Pattern - {{ pattern .Type }}
  {{- end }}
  {{- if format .Type }}
    *    Format - {{ format .Type }}
  {{- end }}
  {{- if .Type.Schema.Schema.MinLength }}
    *    MinLength - {{ .Type.Schema.Schema.MinLength }}
  {{- end }}
  {{- if .Type.Schema.Schema.MaxLength }}
    *    MaxLength - {{ .Type.Schema.Schema.MaxLength }}
  {{- end }}
{{- end -}}

{{- define "renderParamAttributeHelp" -}}
  {{ .Description }}
{{- end -}}

{{- define "renderObject" -}}
/**
 * {{ .Schema.Schema.Description }}
{{- range .Properties }}
 * @property {{ .Name }} - {{ template "renderObjectAttributeHelp" . }}
 {{- end -}}
 */
export interface {{ .Name }} {
{{- range .Properties }}
  /**
   * {{ template "renderObjectAttributeHelp" . }}
   */
  {{ .Name }}: {{ .Type.Name }},
{{- end }}
};
{{- end }}
